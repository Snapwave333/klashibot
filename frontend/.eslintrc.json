{
  "extends": ["react-app", "react-app/jest"],
  "rules": {
    "no-console": ["warn", { "allow": ["warn", "error", "info"] }],
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": "off"
  },
  "ignorePatterns": [
    "build",
    "dist",
    "node_modules",
    "*.config.js",
    "public"
  ]
}
